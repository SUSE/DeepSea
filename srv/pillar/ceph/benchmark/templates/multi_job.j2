[global]
directory={{ dir }}
filename_format=seqential.$filenum
wait_for_previous

{% for bs in blocksizes %}
[{{ number_of_workers}}w_{{ bs }}_write]
rw=write
filesize={{ filesize }}
blocksize={{ bs }}
numjobs={{ number_of_workers}}

[{{ number_of_workers}}w_{{ bs }}_read]
rw=read
filesize={{ filesize }}
blocksize={{ bs }}
numjobs={{ number_of_workers}}
{% endfor %}
